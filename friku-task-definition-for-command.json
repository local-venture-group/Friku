{
    "family": "friku-for-command",
    "taskRoleArn": "arn:aws:iam::455215502417:role/ECSTaskRole",
    "executionRoleArn": "arn:aws:iam::455215502417:role/ECSTaskRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "friku",
            "image": "455215502417.dkr.ecr.ap-northeast-1.amazonaws.com/friku/back:latest",
            "essential": true,

            "environment": [
                {
                    "name": "APP_ENV",
                    "value": "production"
                }
            ],
            "secrets": [
                {
                    "name" : "APP_NAME",
                    "valueFrom" : "arn:aws:ssm:ap-northeast-1:455215502417:parameter/APP_NAME"
                }
            ],
            "readonlyRootFilesystem": false,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "friku-for-command-logs",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-datetime-format": "%Y-%m-%dT%H:%M:%S",
                    "awslogs-stream-prefix": "friku"
                }
            }

        }
    ],

    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"

}